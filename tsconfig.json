{
  "compilerOptions": {
    "target": "es6",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "lib": ["es6", "es2017", "DOM"],
    "module": "commonjs", 
    "strict": true,      
    "esModuleInterop": true, 
    "skipLibCheck": true,   
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
  }
}
